{
  "name": "iobroker.zwave2",
  "version": "0.0.1",
  "description": "Alternative Z-Wave implementation",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.zwave2",
  "license": "MIT",
  "keywords": [
    "zwave",
    "z-wave"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.zwave2"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^1.0.3",
    "zwave-js": "github:AlCalzone/node-zwave-js#master"
  },
  "devDependencies": {
    "@fimbul/mithotyn": "^0.21.0",
    "@fimbul/wotan": "^0.21.1",
    "@iobroker/testing": "^1.2.5",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/gulp": "^4.0.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.7.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --opts test/mocha.custom.opts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "coverage": "nyc npm run test:ts",
    "lint:ts": "eslint --ext .ts src",
    "lint:wotan": "wotan",
    "lint": "npm run lint:ts && npm run lint:wotan",
    "update_iob": "start update_iob.bat"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.zwave2/issues"
  },
  "readmeFilename": "README.md"
}
