{
  "name": "iobroker.zwave2",
  "version": "1.10.1",
  "description": "A better Z-Wave integration for ioBroker",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.zwave2",
  "license": "MIT",
  "keywords": [
    "zwave",
    "z-wave",
    "iobroker",
    "adapter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.zwave2"
  },
  "funding": {
    "url": "https://github.com/sponsors/AlCalzone/"
  },
  "main": "build/main.js",
  "files": [
    "build/",
    "admin/build/",
    "admin/script/",
    "admin/*.{html,css,js,png,svg}",
    "LICENSE",
    "io-package.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.0",
    "alcalzone-shared": "^3.0.4",
    "fs-extra": "^9.1.0",
    "zwave-js": "^7.5.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^1.10.0",
    "@iobroker/testing": "^2.4.4",
    "@tsconfig/node10": "^1.0.7",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.3",
    "@types/d3": "^6.7.0",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/gulp": "^4.0.8",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/materialize-css": "^1.0.9",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.3",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "d3": "^6.7.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "estrella": "^1.4.0",
    "gulp": "^4.0.2",
    "iobroker-react-components": "^1.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.19",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:react": "node .build.js -react",
    "build:ts": "node .build.js -typescript",
    "check:ts": "tsc -p tsconfig.build.json --noEmit",
    "build": "node .build.js -typescript -react",
    "watch:react": "npm run build:react -- --watch",
    "watch:ts": "npm run build:ts -- --watch",
    "watch": "npm run build -- --watch",
    "test:ts": "jest",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "coverage": "nyc npm run test:ts",
    "lint:ts": "eslint --ext .ts src",
    "lint": "npm run lint:ts",
    "update_iob": "start update_iob.bat",
    "start": "NO_CACHE=true LOGLEVEL=debug node --inspect build/main.js --force --logs",
    "release": "release-script"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.zwave2/issues"
  },
  "readmeFilename": "README.md"
}
