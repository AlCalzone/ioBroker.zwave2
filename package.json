{
  "name": "iobroker.zwave2",
  "version": "0.8.0",
  "description": "Alternative Z-Wave implementation",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.zwave2",
  "license": "MIT",
  "keywords": [
    "zwave",
    "z-wave"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.zwave2"
  },
  "main": "build/main.js",
  "files": [
    "build/",
    "admin/build/",
    "admin/script/",
    "admin/*.{html,css,js,png,svg}",
    "LICENSE",
    "io-package.json"
  ],
  "dependencies": {
    "@iobroker/adapter-core": "^2.1.0",
    "alcalzone-shared": "^2.2.0",
    "fs-extra": "^8.1.0",
    "zwave-js": "^2.8.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^1.1.0",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@fimbul/mithotyn": "^0.21.0",
    "@fimbul/wotan": "^0.21.1",
    "@iobroker/testing": "^2.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/d3": "^5.7.2",
    "@types/d3-scale-chromatic": "^1.3.1",
    "@types/fs-extra": "^8.0.1",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.23",
    "@types/jquery": "^3.3.29",
    "@types/materialize-css": "^1.0.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "d3": "^5.11.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^4.0.1",
    "iobroker-react-components": "^1.0.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "mocha": "^7.0.0",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build --public-url .",
    "build:ts": "tsc -p tsconfig.build.json",
    "check:ts": "tsc -p tsconfig.build.json --noEmit",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build --public-url .",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "jest",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "coverage": "nyc npm run test:ts",
    "lint:ts": "eslint --ext .ts src",
    "lint:wotan": "wotan",
    "lint": "npm run lint:ts && npm run lint:wotan",
    "update_iob": "start update_iob.bat",
    "start": "NO_CACHE=true LOGLEVEL=debug node --inspect build/main.js --force --logs",
    "release": "release-script"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.zwave2/issues"
  },
  "readmeFilename": "README.md"
}
