{
  "name": "iobroker.zwave2",
  "version": "1.7.8",
  "description": "A better Z-Wave integration for ioBroker",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "homepage": "https://github.com/AlCalzone/ioBroker.zwave2",
  "license": "MIT",
  "keywords": [
    "zwave",
    "z-wave",
    "iobroker",
    "adapter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlCalzone/ioBroker.zwave2"
  },
  "funding": {
    "url": "https://github.com/sponsors/AlCalzone/"
  },
  "main": "build/main.js",
  "files": [
    "build/",
    "admin/build/",
    "admin/script/",
    "admin/*.{html,css,js,png,svg}",
    "LICENSE",
    "io-package.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.4.0",
    "alcalzone-shared": "^3.0.0",
    "fs-extra": "^9.0.1",
    "zwave-js": "^5.3.3"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^1.8.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.12.1",
    "@iobroker/testing": "^2.3.0",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/d3": "^5.16.3",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/gulp": "^4.0.7",
    "@types/jest": "^26.0.15",
    "@types/jquery": "^3.5.4",
    "@types/materialize-css": "^1.0.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/sinon": "^9.0.7",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "axios": "^0.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "d3": "^6.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "iobroker-react-components": "^1.0.0",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "proxyquire": "^2.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "cross-env NODE_ENV=production parcel build admin/src/index.tsx -d admin/build --public-url .",
    "build:ts": "tsc -p tsconfig.build.json",
    "check:ts": "tsc -p tsconfig.build.json --noEmit",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build --public-url .",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "jest",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "coverage": "nyc npm run test:ts",
    "lint:ts": "eslint --ext .ts src",
    "lint": "npm run lint:ts",
    "update_iob": "start update_iob.bat",
    "start": "NO_CACHE=true LOGLEVEL=debug node --inspect build/main.js --force --logs",
    "release": "release-script"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/ioBroker.zwave2/issues"
  },
  "readmeFilename": "README.md"
}
